<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema  elementFormDefault="qualified" version="1.0" 
			targetNamespace="http://societies.org/comm/examples/calculatorbean" 
			xmlns:tns="http://societies.org/comm/examples/calculatorbean" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="calcBean">
	  <xs:complexType >
	    <xs:sequence>
	      <xs:element name="a" type="xs:int"/>
	      <xs:element name="b" type="xs:int"/>
	      <xs:element name="method" type="tns:methodType" minOccurs="0"/>
	    </xs:sequence>
	  </xs:complexType>
	</xs:element>
	
	<xs:element name="calcBeanResult">
	  <xs:complexType >
	    <xs:sequence>
	      <xs:element name="result" type="xs:int"/>
	      <xs:element name="text" type="xs:string" minOccurs="0"/>
	    </xs:sequence>
	  </xs:complexType>
	</xs:element>

	<xs:simpleType name="methodType">
	  <xs:restriction base="xs:string">
	    <xs:enumeration value="Add"/>
	    <xs:enumeration value="Subtract"/>
	    <xs:enumeration value="AddAsync"/>
	  </xs:restriction>
	</xs:simpleType>
</xs:schema>

